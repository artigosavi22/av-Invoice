{
    "openapi":"3.0.0",
    "info":{
        "title":"Invoice",
        "version":"",
        "description":"SwaggerDucumentation for CrowdSourcing"
    },
    "tags":[
        {
            "name":"Public"
        },
        {
             "name":"OAuth"
        },
        {
             "name":"User"
        },
        {
             "name":"Admin"
        }
    ],
    "components":{
        "securitySchemes":{
            "bearerAuth":{
                "type":"http",
                "scheme":"bearer",
                "bearerFormat":"JWT"

            }
        }
    },
    "path":{
        "/public/country":{
            "get":{
                "tags":["Public"],
                "Summary":"Country list",
                "description":"list of all countries",
                "parameters":[
                    {
                        "in":"query",
                        "name":"language",
                        "example":"en",
                        "schema":{
                            "type":"string"
                        },
                        "required":true
                    },
                    {
                        "in":"query",
                        "name":"tenat_id",
                        "example":1,
                        "schema":{
                            "type":"number"
                        },
                        "required":true
                    }
                ],
                "responses":{
                    "200":{
                        "description":"list of all countries",
                        "content":{
                            "application/json":{
                                "schema":{
                                    "type":"object",
                                    "properties":{
                                        "success":{
                                            "type":"boolean"
                                        },
                                        "status":{
                                             "type":"number"
                                        },
                                        "data":{
                                            "type":"array",
                                            "items":{
                                                "type":"object",
                                                "properties":{
                                                    "country_id":{
                                                        "type":"number"
                                                    },
                                                    "tenant_id":{
                                                        "type":"number"
                                                    },
                                                    "country_short_code":{
                                                        "type":"string"
                                                    },
                                                    "country_code":{
                                                        "type":"number"
                                                    }
                                                }
                                            }
                                        },
                                        "message":{
                                            "type":"string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },


        "/auth/login":{
            "post":{
                "tags":["OAuth"],
                "Summary":"Login",
                "description":"User Login",
                "parameters":[
                    {
                        "in":"query",
                        "name":"language",
                        "example":"en",
                        "schema":{
                            "type":"string"
                        },
                        "required":true
                    }
                ],
                "requestBody":{
                    "required":true,
                    "content":{
                        "application/json":{
                            "schema":{
                                "type":"object",
                                "properties":{
                                    "email":{
                                        "type":"string"
                                    },
                                    "password":{
                                        "type":"string"
                                    },
                                    "tenant_id":{
                                        "type":"number"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses":{
                    "200":{
                        "description":"Login user",
                        "content":{
                            "application/json":{
                                "schema":{
                                    "type":"object",
                                    "properties":{
                                        "success":{
                                            "type":"boolean"
                                        },
                                        "status":{
                                             "type":"number"
                                        },
                                        "data":{
                                            "type":"array",
                                            "items":{
                                                "type":"object",
                                                "properties":{
                                                    "user_id":{
                                                        "type":"number"
                                                    },
                                                    "user_name":{
                                                        "type":"string"
                                                    },
                                                    "email":{
                                                        "type":"string"
                                                    },
                                                    "mobile":{
                                                        "type":"number"
                                                    }
                                                }
                                            }
                                        },
                                        "message":{
                                            "type":"string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },

        "/auth/logout":{
            "post":{
                "tags":["OAuth"],
                "Summary":"Logout",
                "description":"User Logout",
                "parameters":[
                    {
                        "in":"query",
                        "name":"language",
                        "example":"en",
                        "schema":{
                            "type":"string"
                        },
                        "required":true
                    }
                ],
                "security":[
                    {"bearerAuth":[]}
                ],
                "responses":{
                    "200":{
                        "description":"Logout user",
                        "content":{
                            "application/json":{
                                "schema":{
                                    "type":"object",
                                    "properties":{
                                        "success":{
                                            "type":"boolean"
                                        },
                                        "status":{
                                             "type":"number"
                                        },
                                        "data":{
                                            "type":"object"
                                        },
                                        "message":{
                                            "type":"string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },

        
        "/auth/upload_files":{
            "post":{
                "tags":["OAuth"],
                "Summary":"upload files from formdata fields",
                "description":"User Logout",
                "parameters":[
                    {
                        "in":"query",
                        "name":"language",
                        "example":"en",
                        "schema":{
                            "type":"string"
                        },
                        "required":true
                    }
                ],
                "security":[
                    {"bearerAuth":[]}
                ],
                "requestBody":{
                    "required":true,
                    "content":{
                        "multipart/form-data":{
                            "schema":{
                                "type":"object",
                                "properties":{
                                    "project_id":{
                                        "type":"number"
                                    },
                                    "project_name":{
                                        "type":"string"
                                    },
                                    "document_path_upload_file":{
                                        "type":"string",
                                        "format":"binary",
                                        "description":"file to uload"
                                    }
                                },
                                "required":["project_id","project_name"]
                            }
                        }
                    }
                },
                "responses":{
                    "200":{
                        "description":"Logout user",
                        "content":{
                            "application/json":{
                                "schema":{
                                    "type":"object",
                                    "properties":{
                                        "success":{
                                            "type":"boolean"
                                        },
                                        "status":{
                                             "type":"number"
                                        },
                                        "data":{
                                            "type":"object"
                                        },
                                        "message":{
                                            "type":"string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }


    }
}